package model.cell;

import java.util.ArrayList;

import model.gem.Gem;

public abstract class Cell {
	ArrayList<Gem> gemInCell = new ArrayList<Gem>(); 
	private int value;
	
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}
	
	public void showCell() {
		for (Gem gem : gemInCell) {
			gem.showGem();
		}
	}
}
